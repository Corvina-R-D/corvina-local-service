{
  "name": "CorvinaLocalServer",
  "version": "1.1.5",
  "description": "Local proxy interface for corvina APIs",
  "main": "main/dist/main.js",
  "scripts": {
    "build": "cd main && vite build",
    "build:swagger": "cd main && nestia swagger",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "build:types": "typia generate --input ./main/src/templates/ --output ./main/src/generated --project ./main/src/tsconfig.json",
    "start:dev": "cross-env NODE_ENV=development MODE=dev  electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prepare": "ts-patch install"
  },
  "keywords": [],
  "author": "Arrigo Zanette <arrigo.zanette@corvina.io>",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-vite": "^6.1.1",
    "@electron-forge/publisher-bitbucket": "^6.1.1",
    "@electron/remote": "^2.0.9",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@nestia/sdk": "^1.1.0",
    "@types/express": "^4.17.17",
    "@types/jwt-decode": "^3.1.0",
    "@types/keytar": "^4.4.2",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@types/qs": "^6.9.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cross-env": "^7.0.3",
    "electron": "^24.1.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "nestia": "^4.1.1",
    "prettier": "^2.8.8",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@nestia/core": "^1.1.1",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "electron-squirrel-startup": "^1.0.0",
    "express": ">=5.0.0-beta.1",
    "form-data": "^4.0.0",
    "i": "^0.3.7",
    "jwt-decode": "^3.1.2",
    "keytar": "^7.9.0",
    "node-abi": "^3.40.0",
    "node-cache": "^5.1.2",
    "qs": "^6.11.1",
    "semver": "^7.5.0",
    "semver-diff": "^4.0.0",
    "swagger-ui-express": "^4.6.2",
    "typia": "^3.8.1",
    "win-ca": "^3.5.0"
  }
